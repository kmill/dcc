class Program {
  int x[10];
  int y[20];

  void main() {

    for (i = 0; 10) {
      x[i] = 2 * i;
      y[2 * i] = 20 - 2 * i;
      y[2 * i + 1] = x[i];
    }

    callout("printf", "x[4]: %d\n", x[4]);
    callout("printf", "y[8]: %d\n", y[8]);
    callout("printf", "y[x[4]]: %d\n", y[x[4]]);
  }
}
